        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/gray_7</item>
    </style>

    <style name="textViewListEmpty">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/gray_7</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="textViewLocation">
        <item name="android:textSize">12sp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="textViewLocationLayout">
        <item name="android:gravity">center</item>
        <item name="android:background">@color/gray_7</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
    </style>

    <style name="textViewInterest">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/gray_3</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_gravity">center_vertical|right</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:background">@drawable/ic_interest_bg</item>
    </style>

    <style name="filter_sort_layout_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="filter_button_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="filter_subtitle_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/gray_f</item>
        <item name="android:padding">8dp</item>
    </style>

    <style name="filter_sort_image_style">
        <item name="android:layout_width">32dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:padding">5dp</item>
        <!--<item name="android:tint">@color/gray_9</item>-->
        <item name="tint">@color/selector_image_like</item>
    </style>

    <style name="filter_sort_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
        <item name="android:textColor">@color/selector_image_like</item>
        <!--<item name="android:textColor">@color/gray_9</item>-->
    </style>

    <style name="filter_checkbox_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">5dp</item>
        <!--<item name="android:textColor">@color/selector_image_like</item>-->
        <item name="android:textColor">@color/gray_7</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="filter_checkbox_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text"></item>
        <item name="android:clickable">false</item>
    </style>

    <!-- For checkbox, use as theme. Currently not required, as buttonTint does the same work -->
    <style name="checkBoxStyle">
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <item name="android:textColorSecondary">@color/colorPrimary</item>
    </style>

    <style name="filter_checkbox_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">9dp</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

    <!-- Edittext -->
    <style name="normal_edittext_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/gray_d</item>
        <item name="android:textCursorDrawable">@drawable/text_cursor_gray_3</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/post_background</item>
        <item name="android:padding">9dp</item>
    </style>

    <style name="normal_edittext_nopadding_style" parent="normal_edittext_style">
        <item name="android:background">@drawable/edittext_background_nopadding</item>
    </style>

    <style name="login_edittext_style" parent="normal_edittext_style">
        <item name="android:background">@drawable/edittext_login</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textColorHint">@color/blue_grey</item>
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textCursorDrawable">@null</item>
    </style>

    <style name="facebook_text_style">
        <item name="android:textSize">@dimen/login_btn_text_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="core_edittext_style" parent="normal_edittext_style">
        <item name="android:textColor">@color/black_text</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@null</item>
        <item name="android:padding">2dp</item>
        <item name="android:inputType">textCapSentences</item>
    </style>

    <style name="core_edittext_style_textview" parent="normal_edittext_style">
        <item name="android:textColor">@color/black_text</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@null</item>
        <item name="android:padding">2dp</item>
    </style>

    <style name="core_edittext_style2" parent="core_edittext_style">
        <item name="android:textColor">@color/gray_text</item>
        <item name="android:textColorHint">@color/gray_9</item>
    </style>

    <style name="core_edittext_style3" parent="core_edittext_style">
        <item name="android:textColor">@color/off_white</item>
        <item name="android:textColorHint">@color/gray_text</item>
    </style>

    <style name="core_edittext_style4" parent="core_edittext_style">
        <item name="android:textColor">@color/black_text</item>
        <item name="android:textColorHint">@color/gray_text_hint</item>
        <item name="android:background">@drawable/background_light_grey</item>
        <item name="android:textSize">18sp</item>
    </style>

    <!--- Spinner -->
    <style name="spinner_normal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- Line -->
    <style name="line_black">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.7sp</item>
        <item name="android:background">@color/black</item>
    </style>

    <style name="line_blue_grey" parent="line_black">
        <item name="android:background">@color/blue_grey</item>
    </style>

    <style name="line_grey" parent="line_black">
        <item name="android:background">@color/gray_text</item>
    </style>

    <style name="line_shadow" parent="line_black">
        <item name="android:layout_height">1.5sp</item>
        <item name="android:background">@drawable/ic_shadow</item>
    </style>

    <style name="line_green" parent="line_black">
        <item name="android:layout_height">1sp</item>
        <item name="android:background">@color/line_green</item>
    </style>

    <style name="DialogScale" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowAnimationStyle">@style/DialogScaleAnimation</item>
    </style>

    <style name="DialogScaleAnimation">
        <item name="android:windowEnterAnimation">@anim/scale_more</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>
</resources>
